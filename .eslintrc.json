{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-unused-vars": "off",
        "quotes": ["warn", "double"],
        "linebreak-style": ["error", "windows"],
        "spaced-comment": ["warn", "always"],
        "max-len": "off",
        "comma-dangle": ["warn", "never"],
        "keyword-spacing": "off",
        "new-cap": "off",
        "require-jsdoc": "off",
        "semi": "warn",
        "brace-style": "warn",
        "padded-blocks": "off",
        "prefer-const": "warn"
    }
}